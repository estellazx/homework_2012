set xlabel "Recall"
set ylabel "Precision"

set terminal postscript color
set size 0.75,0.75

set data style linespoints
set key 0.75,0.9

set xrange [0:1]
set yrange [0:1]

plot 'rp0' title "N = 0", 'rp1' title "N = 1", 'rp2' title "N = 2", 'rp4' title "N = 4", 'rp6' title "N = 6", 'rp8' title "N = 8", 'rp10' title "N = 10"
